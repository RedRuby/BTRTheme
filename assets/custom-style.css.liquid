{% assign lite-accent = settings.color_primary %}
{% assign dark-accent = settings.color_footer_bg %}

body {
    padding-top: 0px;
}

body, html{
	overflow-x: hidden;
}

.wrapper{
	max-width:100%;
}

.template-index main{
	padding: 0px;
}

.template-index .shopify-section.collection-list-section{
	padding: 0 70px 50px;
}

.shipping-alert-banner{
    color: #ffffff;
    font-size: 13px;
    font-style: normal;
    font-stretch: normal;
    font-weight: 400;
    line-height: 18px;
    text-align: center;
    text-transform: none;
    text-decoration: none;
    letter-spacing: 0px;
  	padding: 16px 166px;
  	transform: all 0.5s;
  	-webkit-transform: all 0.5s;
}

@media(max-width: 1443px){
  .shipping-alert-banner br{
  	display: none;
  }
}

@media(max-width: 1200px){
  .shipping-alert-banner{
  	padding: 16px 70px;
  }
}

@media(max-width: 768px){
  .shipping-alert-banner span{
  	top: 16px !important;
    right: 16px !important;
  }
  
  .shipping-alert-banner{
  	padding: 16px 40px;
  }
}


.header-logo {
    margin: 15px 0 !important;
}

.site-header{
	padding:16px 0 !important;
}

.header-cart-btn{
	padding: 8px;
    color: {{ dark-accent }} !important;
  	background-color: transparent !important;
  	vertical-align: middle;
}

.cart-count{ background-color: {{ lite-accent }} !important; font-size:10px; }

.site-nav a, .site-nav button{
	padding:10px;
}

.nav-bar{
/* 	-webkit-box-shadow: 0 6px 6px -6px #333;
    -moz-box-shadow: 0 6px 6px -6px #333;
    box-shadow: 0 6px 6px -6px #333; */
    /* box-shadow: 0 5px 5px -5px #333; */
}

.site-nav--link .fa-caret-down, .mobile-nav--link .fa-caret-down{
	opacity: 0.5;
}

.nav-hover .fa-caret-down, .nav-active .fa-caret-down, [aria-expanded="true"] .fa-caret-down{
	display: none !important;
}

.nav-hover .fa-caret-up, .nav-active .fa-caret-up, [aria-expanded="true"] .fa-caret-up{
	display: inline-block !important;
}

.site-nav--dropdown {
    background-color: {{ dark-accent }};
  	border-color: {{ dark-accent }};
  	padding:10px;
  	border-bottom-left-radius: 5px;
  	border-bottom-right-radius: 5px;
  	min-width: 288px;
}

.w3-right .site-nav--dropdown {
  	min-width: 100%;
  	margin-left:10px;
}

.mobile-nav{
	text-align: left;
}

.mobile-nav--dropdown{
	background-color: {{ dark-accent }};
}

.mobile-nav--dropdown>li, .mobile-nav--dropdown-grandchild>li{ border: none; }

.mobile-nav li.mobile-nav--has-dropdown.nav-active a{ background-color: transparent; }

.site-nav--dropdown a, .site-nav--dropdown-grandchild a{
	color: white !important;
    background-color: {{ dark-accent }} !important;
  	padding: 5px;
}

.site-nav--dropdown a:hover, .site-nav--dropdown-grandchild a:hover{
	opacity:0.7;
}

.breadcrumb{
	margin-bottom: 0px;
  	margin-top: -20px;
}

.breadcrumb *{
	color: {{ dark-accent | color_modify: 'alpha', 0.6 }} !important;
  	font-size: 11px !important;
  	margin-right: 0px !important;
}

.breadcrumb b{
  	font-weight: normal !important;
  	color: {{ dark-accent }} !important;
}

.breadcrumb a{
	padding: 15px 0px 15px 0;
}

.breadcrumb .divider{
  	padding: 15px 0px 15px 0;
}

.breadcrumb *:hover{
	color: {{ dark-accent }} !important;
}

.featured-box--title{
	color: {{ dark-accent }} !important;
  	font-style: italics !important; 
/*     font-weight: bolder !important; */
  	font-size: 20px;
/*   	text-transform: uppercase; */
}

.featured-box{
	border: none;
  	border-radius: 5px;
}

.featured-box:after{
	background: transparent;
}

main{
	min-height: calc(99vh - 215px);
}

.template-customers-account main{
	min-height: calc(99vh - 203px);
}

.customer-form-item-wrapper{
	max-width:350px;
  	margin:auto;
}
/* -----------------------Body main---------------------------- */

.btn--secondary{
	background-color: white;
  	border: 2px solid {{ lite-accent }};
  	color: {{ lite-accent }};
  	border-radius: 5px;
  	font-weight: normal !important;
}

.btn:not(.btn--secondary), input:not([type="checkbox"]), select, textarea{
	max-width: 350px !important;
  	width: 100%;
  	height: 60px;
  	border-radius: 5px;
  	padding-left:15px;
  	padding-right:15px;
}

.btn:not(.btn--secondary):disabled{
    background-color: {{ lite-accent | color_modify: 'alpha', 0.5 }};
  	color: white;
}

.btn:not(.btn--secondary){ font-style: italics !important; font-weight: bold !important; }

.btn2{
	background-color: {{ dark-accent | color_modify: 'alpha', 0.2 }} !important;
    color: {{ dark-accent }} !important;
  	font-weight: normal !important;
  	font-style: normal !important;
  	font-size: 110%;
  	text-transform: none;
}

.btn2.error{
	background-color: {{ '#F44336' | color_modify: 'alpha', 0.5 }} !important;
    color: white !important;
}

*{ outline: none !important; letter-spacing: 0.3px; }

input:not(.btn), select, textarea{
	background-color: white !important;
  	border: 1px solid {{ dark-accent | color_modify: 'alpha', 0.5 }} !important;
  	color: {{ dark-accent | color_modify: 'alpha', 0.5 }}; 
  	font-size:16px;
}

input:not(.btn):enabled:focus, select:enabled:focus, textarea:enabled:focus{
  	border: 1px solid {{ dark-accent }} !important;
  	transition: all 0.5s ease;
  	-webkit-transition: all 0.5s ease;
  	box-shadow: 0 2px 4px 0 rgba(0,0,0,0.16),0 2px 10px 0 rgba(0,0,0,0.12);
  	color: {{ dark-accent }};
}

select, .autocomplete{
    background-image: url('{{ "ico-select.svg" | asset_url }}');
    background-position:right 15px center;
}

label{ font-weight: bolder; }

.form-wrap{
	background-color: white;
  	border-radius: 10px;
  	padding: 50px;
  	margin: 60px 0 25px 15px;
  	max-width: 450px !important;
}

.text-lite-blue, .text_Fulfilled{
    color: {{ lite-accent }} !important;
}

.text_Unfulfilled, .text_Cancelled, .w3-text-red{
	color: #be3326 !important;
}

.text-liter-blue{
    color: {{ dark-accent | color_modify: 'alpha', 0.5 }} !important;
}

.text-dark-blue, label, .account-wrapper .w3-medium{
	color: {{ dark-accent }} !important;
}

.bg-lite-blue{
    background-color: {{ lite-accent }} !important;
}

.bg-dark-blue{
	background-color: {{ dark-accent }} !important;
}

.border-lite-blue{
	border-color: {{ lite-accent }} !important;
}

.border-liter-blue, .table-border-liter-blue td, .table-border-liter-blue th{
	border-color: {{ dark-accent | color_modify: 'alpha', 0.4 }} !important;
}

.border-dark-blue, .mobile-nav li, .mobile-nav{
	border-color: {{ dark-accent | color_modify: 'alpha', 0.7 }} !important;
}

.text-gray{
	color: gray !important;
}

sup{
	top: 0;
  	font-size: 100%;
}

sup::before{
	content: '.';
}

/* -----------------------Collection---------------------------- */
.collection-wrapper{
	padding: 0 70px 0 50px;
}

.collection-wrapper .grid-uniform .grid-item{
	padding-left: 45px;
}

#ProductSection{
	padding: 0 110px;
}

.product-grid-image--centered, .product-photo-container, .cart-image{
	border: 1px solid {{ dark-accent | color_modify: 'alpha', 0.5 }};
  	border-radius: 5px;
  	padding: 10px;
}

/* ---------------------------Product----------------------------- */

h1, .h1, #shopify-product-reviews .spr-header-title{
	font-size: 32px;
}

.product-description.rte{
	margin:15px 0;
  	color: {{ dark-accent }};
    line-height:22px;
  	font-size:16px;
}

.js--qty-adjuster, .ajaxifyCart--qty-adjuster{
	padding: 18px 0;
  	background: transparent !important;
}

.single-option-selector{
	max-width:555px !important;
}

.payment-buttons.payment-buttons--medium {
    min-width: auto;
    width: 100%;
}

.w3-opacity-25{
	opacity:0.25 !important;
}

.field_tees_true .btn{
	max-width:48% !important;
}

/* -------------------------Account-------------------------- */
.mob-font{
	text-transform: none;
  	font-size: 85% !important;
}

.allowance-btn{
	padding: 8px 0px !important;
}

.account-wrapper{
	padding:0 50px;
}

.account-wrapper th, .account-wrapper td{
	padding:15px 0;
  	border:none;
  	border-bottom: 1px {{ dark-accent | color_modify: 'alpha', 0.7 }} solid;
}

.account-wrapper th{
  	padding:6px 0;
  	color: {{ dark-accent | color_modify: 'alpha', 0.4 }} !important;
  	border-bottom: 1px {{ dark-accent | color_modify: 'alpha', 0.4 }} solid;
}

.null-placeholder{
	background:url('{{ 'minus-icon.svg' | asset_url }}') no-repeat;
}
/* -------------------------Footer-------------------------- */

.site-footer{
	padding: 10px 0 !important;
}

.legal-links li{
    font-size:16px; /*{{ settings.type_base_size }};*/
}

.inline-block{ display: inline-block !important; }


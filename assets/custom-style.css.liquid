@media (max-width: 768px && min-width: 481px){
  .medium--hide {
    display: none;
  }
  .medium--one-whole{
  	width:100%;
  }
}

@media (max-width: 768px){/*480px*/
  .site-header:not(.no-cart-header) .grid--full>.grid-item {
    float: none;
    display: table-cell;
    vertical-align: middle;
  }
  .btn:not(.btn--secondary), input:not([type="checkbox"]), select, textarea {
    max-width: 100% !important;
  }
  .w3-small-center{ position: relative !important; }
}

@media (max-width: 386px){
  .mob-font{
  	font-size: 70% !important;
    font-weight: normal !important;
  }
  .allowance-btn{
  	max-width: 150px !important;
  }
}

.middleXY{
  	position: absolute;
	transition: .1s ease;
    top: 50% !important;
    left: 50% !important;
    transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
}
  
.middleX{
  	position: absolute;
	transition: .1s ease;
    left: 50% !important;
    transform: translateX(-50%);
    -ms-transform: translateX(-50%);
    -webkit-transform: translateX(-50%);
}
  
.middleY{
  	position: absolute;
	transition: .1s ease;
    top: 50% !important;
    transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
}

.w3-bold{
	font-weight: 700 !important;
}

.w3-uppercase{
	text-transform: uppercase !important;
}

.w3-medium{
  	font-size: 14px !important;
}

.wrapper.admin-tab .w3-medium, .font-16, form label{
  	font-size: 16px !important;
}

table.text-center th, table.text-center td{ text-align:center; }

table.text-center tr th:first-of-type, table.text-center tr td:first-of-type{
	border-left:none;
} 

table.text-center tr th:last-of-type, table.text-center tr td:last-of-type{
	border-right:none;
} 

a{
	cursor: pointer !important;
}

*[disabled]:not(.allowance-btn), *:disabled:not(.allowance-btn){
	cursor: not-allowed !important;
}

.blockUI{z-index:2000 !important;}

.pagination-custom>.active>a, .pagination-custom>.active>span, .pagination-custom>.active>a:hover, .pagination-custom>.active>span:hover, .pagination-custom>.active>a:focus, .pagination-custom>.active>span:focus{
	color:white;
    background-color:{{ lite-accent }};
    border:1px {{ lite-accent }} solid;
}

select:focus, input.autocomplete:focus{
	border-bottom-left-radius:0px !important;
  	border-bottom-right-radius:0px !important;
}

.w3-pagination li a{
  	margin-top: 5px;
	margin-right: 5px;
  	border-radius: 5px;
  	font-weight: 700;
  	border: 2px {{ lite-accent }} solid !important;
}

/* .w3-pagination li:nth-of-type(1)  a, .w3-pagination li:nth-last-of-type(1) a{
	padding: 10.5px 14px;
} */

.w3-pagination li span{
  	margin-top: 5px;
	margin-right: 5px;
  	border-radius: 5px;
  	padding: 5px 12px 11px;
  	font-weight: 700;
  	border: 2px {{ lite-accent }} solid !important;
}

.w3-pagination li a:not(.current){
	color: {{ lite-accent }};
}

.w3-pagination li a.current{
	cursor: default !important;
}

.w3-pagination li:not(.disabled):hover a:not(.current){
	background-color: {{ lite-accent }};
  	color: white;
}

.w3-pagination li.disabled a{
	color: {{ dark-accent | color_modify: 'alpha', 0.4 }} !important;
  	border: 2px {{ dark-accent | color_modify: 'alpha', 0.4 }} solid !important;
  	cursor: not-allowed !important;
}

.autocomplete{
    background-repeat: no-repeat;
    background-position: right 10px center;
}

.ui-autocomplete{
	background-color: white;
    border: 1px {{ dark-accent }} solid;
  	border-bottom-left-radius:5px;
  	border-bottom-right-radius:5px;
  	max-width:200px !important;
  	border-top:none;
  	box-shadow: 0 2px 4px 0 rgba(0,0,0,0.16),0 2px 10px 0 rgba(0,0,0,0.12)!important
}

.ui-menu-item{
  	font:inherit !important;
	padding:5px 10px 0;
  	cursor:pointer;
  	color:{{ dark-accent | color_modify: 'alpha', 0.6 }};
}

.ui-menu-item:hover{
    color:{{ dark-accent }};
}

.w3-wait{
	cursor:wait;
}

.ui-helper-hidden-accessible{
	display: none;
}

*.cancelled {
    text-decoration: line-through !important;
    color: gray !important;
}

select{ margin: 0 0 20px; }

#cffPCbootstrapContainer .modalLongButton {
    margin-top: 5px;
    margin-bottom: 5px;
  	width: 100% !important;
    max-width: 100% !important;
    white-space:normal;
}

/* ______________ */

.btn-group button, .cross-btn {
    background-color: transparent !important; /* Green background */
    border: 1px solid transparent !important; /* Green border */
  	outline: none !important;
  	font-size:19px !important;
    color: grey; /* White text */
    padding: 2px; /* Some padding */
    cursor: pointer; /* Pointer/hand icon */
    width: 15px !important; /* Set a width if needed */
  	height: 15px !important;
    display: block; /* Make the buttons appear below each other */
  	position: relative;
  	line-height:1 !important;
/*   	vertical-align:middle; */
}

@-moz-document url-prefix() { 
  .btn-group button, .cross-btn {
     bottom:5px;
  }
}

.btn-group, .cross-btn { margin:0px 8px;display:inline-block; }

.cross-btn {  height:10px;}
/* .btn-group { line-height:1; } */

.btn-group button:not(:last-child) {
    border-bottom: none; /* Prevent double borders */
}

/* Add a background color on hover */
.btn-group button:hover, .cross-btn:hover, .cross-btn.active {
    color: {{ dark-accent }} !important;
}

.th-span{ position:relative; bottom:10px; }

